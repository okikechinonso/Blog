# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  firstname: String!
  lastname: String!
  email: String!
  password: String!
}

type Query {
  todos: [Todo!]!
}

input NewTodo {
  text: String!
  userId: String!
}

type Mutation {
  createTodo(input: NewTodo!): Todo!
}
